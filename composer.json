{
    "name": "tattali/mobile-detect-bundle",
    "description": "Symfony5/6 bundle for detect mobile devices, managing mobile view types, redirect to mobile version.",
    "keywords": [
        "mobile detect",
        "mobile redirect",
        "mobile view managing",
        "mobile",
        "mobiledetect",
        "mobiledetectbundle",
        "symfony mobile"
    ],
    "homepage": "https://github.com/tattali/MobileDetectBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "ThÃ©o Attali",
            "email": "theoattali@gmail.com"
        },
        {
            "name": "Nikolay Ivlev",
            "email": "nikolay.kotovsky@gmail.com"
        },
        {
            "name": "Henri Vesala",
            "email": "henri.vesala@gmail.com"
        },
        {
            "name": "netmikey",
            "homepage": "https://github.com/netmikey"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "mobiledetect/mobiledetectlib": "^2.8.15",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.4",
        "symfony/event-dispatcher": "^3.4 || ^4.4 || ^5.4",
        "symfony/framework-bundle": "^3.4 || ^4.4 || ^5.4",
        "symfony/yaml": "^3.4 || ^4.4 || ^5.4",
        "twig/twig": "^2.0 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-nette": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/phpunit-bridge": "^3.4 || ^4.4 || ^5.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "MobileDetectBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MobileDetectBundle\\Tests\\": "tests/"
        }
    }
}
